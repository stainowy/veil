fn main() {
    let number = 7;

    if (number > 5) {
        print("Number is greater than 5");
    } else {
        print("Number is less than or equal to 5");
    }

    for (let i = 0; i < 5; i = i + 1) {
        print(i);
    }

    while (number > 0) {
        print(number);
        number = number - 1;
    }
}

